# Example $HOME/.config/gits/repo_locations
#
# Define repository locations in a bash array.
#
# Bash array name naming convention:
# Example:  repo_{name} where {name} is the repository location $HOME/{name}.
#
# Entries are url + space + alias.
# Example: "htttps://github.com/traap/gits gits"

repo_editor=(
  "https://github.com/LazyVim/starter LazyVim"
  "https://github.com/folke/lazy.nvim lazy.nvim"
  "https://github.com/neovim/neovim neovim"
)

repo_fzf=(
  "https://github.com/junegunn/fzf-git.sh fzf"
  "https://github.com/junegunn/everything.fzf everything"
)

repo_hyprland=(
  "https://github.com/JaKooLit/Hyprland-Dots Dots"
  "https://github.com/JaKooLit/Arch-Hyprland Arch"
)

repo_plugins=(
  "https://github.com/nvim-neo-tree/neo-tree.nvim neo-tree"
  "https://github.com/nvim-telescope/telescope.nvim telescope"
  "https://github.com/tpope/vim-fugitive fugitive"
)

repo_traap=(
  "https://github.com/Traap/gits gits"
  "https://github.com/Traap/nvims nvims"
)
